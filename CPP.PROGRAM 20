#include <iostream>
using namespace std;

// Function template
template <class T>
T add(T a, T b) {
    return a + b;
}

int main() {
    cout << "Addition of integers: " << add(10, 20) << endl;
    cout << "Addition of floats: " << add(5.5, 2.3) << endl;
    cout << "Addition of doubles: " << add(10.25, 15.75) << endl;
    return 0;
}

OUTPUT
Addition of integers: 30
Addition of floats: 7.8
Addition of doubles: 26
