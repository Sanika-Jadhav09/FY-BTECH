#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int num,i=1;
    cout<<"enter a number"<<endl;
    cin>>num;
    do{
        cout<<num<<" * "<<i<<" = "<<num*i<<endl;
        i++;
    }while(i<=10);
    return 0;
}



out put
5
5 * 1 = 5
5 * 2 = 10
5 * 3 = 15
5 * 4 = 20
5 * 5 = 25
5 * 6 = 30
5 * 7 = 35
5 * 8 = 40
5 * 9 = 45
5 * 10 = 50


